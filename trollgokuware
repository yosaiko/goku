-- Scripts:

local function COYP_fake_script() -- check1.LocalScript 
	local script = Instance.new('LocalScript', check1)

	script.Parent.Parent.MouseButton1Click:Connect(function()
		script.Parent.Active = true
		script.Parent.Visible = true
	end)
end
coroutine.wrap(COYP_fake_script)()
